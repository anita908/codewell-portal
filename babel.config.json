{
  "presets": [
    "@babel/react",
    [
      "@babel/preset-env",
      {
        "corejs": 3,
        "targets": "defaults",
        "useBuiltIns": "entry"
      }
    ]
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }]
  ],
  "env": {
    "test": {
      "plugins": ["@babel/plugin-transform-runtime"]
    }
  }
}
